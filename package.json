{
  "name": "sherpa-interview",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "ts-node ./src/app.ts",
    "test": "mocha -r ts-node/register ./src/**/*.test.ts",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "jquery": "^3.6.0",
    "jsdom": "^16.5.3",
    "module-alias": "^2.2.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.1",
    "winston": "^3.2.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/find": "^0.2.1",
    "@types/jasmine": "^3.3.15",
    "@types/jquery": "^3.5.5",
    "@types/jsonfile": "^5.0.0",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.6.8",
    "@types/request-promise": "^4.1.44",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.4.0",
    "jsonfile": "^5.0.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "request-promise": "^4.2.4",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
